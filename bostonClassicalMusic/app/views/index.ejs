<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Time</th>
      <th>Venue</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th>Link</th>
    </tr>
  </thead>
  <tbody>
    <% console.log(gigs); %>
    <% gigs.forEach(gig => { %>
    <tr>
      <td><%= gig.date %></td>
      <td><%= gig.time %></td>
      <td><%= gig.venue %></td>
      <td><%= gig.city %></td>
      <td><%= gig.state %></td>
      <td><%= gig.country %></td>
      <td><%= gig.link %></td>
    </tr>
    <% }); %>
  </tbody>
</table>