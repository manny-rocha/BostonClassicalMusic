<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href= "./public/assets/styles.css" rel="stylesheet" type="text/css"/>
  <title>Boston Classical Music Calendar</title>
</head>
<body>
  <table>
    <thead>
      <tr>
        <th>Performer</th>
        <th>Date</th>
        <th>Time</th>
        <th>City</th>
        <th>Notes</th>
        <th>Tickets</th>
        <th>Location</th>
        <th>Calendar</th>
      </tr>
    </thead>
    <tbody>
      <% events.forEach((gig) => { %>
        <tr>
          <td><%= gig.performer %></td>
          <td><%= gig.date %></td>
          <td><%= gig.time %></td>
          <td><%= gig.city %></td>
          <td><%= gig.notes %></td>
          <td><a href="<%= gig.location %>">Location</a></td>
          <td><a href="<%= gig.ticketLink %>"></a>Tickets</td>
          <td><a href="<%= gig.gCalendar %>"></a>Google Cal</td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
